{
  "name": "first-contribution",
  "description": "Automatically respond to a user's first contribution to your repository, using GitHub Actions.",
  "version": "4-beta-1",
  "author": "Paul Ebose",
  "private": true,
  "type": "module",
  "homepage": "https://github.com/plbstl/first-contribution",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plbstl/first-contribution.git"
  },
  "bugs": {
    "url": "https://github.com/plbstl/first-contribution/issues"
  },
  "keywords": [
    "actions",
    "github-actions",
    "contribution",
    "first-time",
    "message",
    "contributor",
    "comment",
    "first-interaction",
    "label"
  ],
  "engines": {
    "node": ">=24"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@swc/core",
      "esbuild",
      "unrs-resolver"
    ]
  },
  "scripts": {
    "format": "prettier --write .",
    "typecheck": "tsc --build",
    "lint": "eslint .",
    "mdlint": "markdownlint .",
    "test": "vitest --run",
    "coverage": "vitest run --coverage",
    "coverage-badge": "pnpm run coverage && ./scripts/coverage-badge",
    "jscpd": "jscpd",
    "jscpd-badge": "pnpm run jscpd --reporters badge",
    "bundle": "rollup --config rollup.config.js",
    "all": "pnpm format && pnpm typecheck && pnpm lint && pnpm mdlint && pnpm test && pnpm coverage-badge && pnpm jscpd-badge && pnpm bundle"
  },
  "license": "MIT",
  "dependencies": {
    "@actions/core": "1.11.1",
    "@actions/github": "6.0.1"
  },
  "devDependencies": {
    "@eslint/js": "9.35.0",
    "@jscpd/badge-reporter": "4.0.1",
    "@octokit/webhooks-types": "7.6.1",
    "@rollup/plugin-commonjs": "28.0.6",
    "@rollup/plugin-node-resolve": "16.0.1",
    "@rollup/plugin-swc": "0.4.0",
    "@rollup/plugin-terser": "0.4.4",
    "@swc/core": "1.13.5",
    "@types/node": "24.3.1",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/eslint-plugin": "1.3.9",
    "badge-maker": "5.0.2",
    "eslint": "9.35.0",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-github": "6.0.0",
    "eslint-plugin-import-x": "4.16.1",
    "globals": "16.3.0",
    "jscpd": "4.0.5",
    "markdownlint-cli": "0.45.0",
    "prettier": "3.6.2",
    "rollup": "4.50.0",
    "typescript": "5.9.2",
    "typescript-eslint": "8.42.0",
    "vitest": "3.2.4"
  },
  "packageManager": "pnpm@10.15.1"
}
